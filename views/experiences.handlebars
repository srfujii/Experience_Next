<div class="container container-fluid mt-5">

<ul>
{{#each expCategory as |experience| }}
<li>
    <div class="row mb-4">
        <div class="col-md-5">
        <h2><a href="/experience/{{experience.id}}">{{experience.provider_name}}</a></h2>
        <p>{{experience.category}} {{experience.city}}</p>
        </div>
    <div class="col-md-7">
    <p>{{experience.description}}</p>
    <p>${{experience.price}}</p>
    </div>
</div>
</li>
{{/each}}
</ul>

</div>